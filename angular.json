{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "systelab-preferences": {
      "projectType": "library",
      "root": "projects/systelab-preferences",
      "sourceRoot": "projects/systelab-preferences/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/systelab-preferences/tsconfig.lib.json",
            "project": "projects/systelab-preferences/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/systelab-preferences/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/systelab-preferences/src/test.ts",
            "tsConfig": "projects/systelab-preferences/tsconfig.spec.json",
            "karmaConfig": "projects/systelab-preferences/karma.conf.js",
            "scripts": [
              "node_modules/pako/dist/pako.min.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/systelab-preferences/tsconfig.lib.json",
              "projects/systelab-preferences/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "defaultProject": "systelab-preferences"
}
