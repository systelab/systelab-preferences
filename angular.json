{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "systelab-preferences": {
      "projectType": "library",
      "root": "projects/systelab-preferences",
      "sourceRoot": "projects/systelab-preferences/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/systelab-preferences/tsconfig.lib.json",
            "project": "projects/systelab-preferences/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/systelab-preferences/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/systelab-preferences/src/test.ts",
            "tsConfig": "projects/systelab-preferences/tsconfig.spec.json",
            "karmaConfig": "projects/systelab-preferences/karma.conf.js",
            "scripts": [
              "node_modules/pako/dist/pako.min.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/systelab-preferences/**/*.ts",
              "projects/systelab-preferences/**/*.html"
            ]
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  },
  "cli": {
    "analytics": false
  }
}
